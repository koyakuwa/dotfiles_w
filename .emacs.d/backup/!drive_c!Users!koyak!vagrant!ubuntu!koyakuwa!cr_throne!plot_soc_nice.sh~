set terminal png
set xlabel "Time [min]"
set ylabel "SOC [%]"
set yrange[0:100]
unset key
set output '/dev/null'
plot "/vagrant/koyakuwa/flightdata/20160708_Throne34_MaxFlightTime/15_12_20/soccc.dat" using 1:2 w l lc rgb "black"
replot "/vagrant/koyakuwa/flightdata/20160810_Throne_016_Fukushima_takenaka_accident/01_32_30/soccc.dat" using 1:2 w l lc rgb "red"
replot "/vagrant/koyakuwa/flightdata/20160823_Throne31_Aktio_Inabe/02_41_58/soccc.dat" using 1:2 w l lc rgb "blue"
# soc by vol
replot "/vagrant/koyakuwa/flightdata/20160708_Throne34_MaxFlightTime/15_12_20/soccc.dat" using 1:3 w l lc rgb "black"
replot "/vagrant/koyakuwa/flightdata/20160810_Throne_016_Fukushima_takenaka_accident/01_32_30/soccc.dat" using 1:3 w l lc rgb "red"
set output 'plot_soc_nice.png'
replot "/vagrant/koyakuwa/flightdata/20160823_Throne31_Aktio_Inabe/02_41_58/soccc.dat" using 1:3 w l lc rgb "blue"
