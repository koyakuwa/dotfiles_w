#!/bin/sh
python replace.py memo_3.tex "。" ". "
python replace.py memo_3.tex "、" ", "
python replace.py memo_3.tex "。" ". "
python replace.py memo_3.tex "、" ", "
platex --kanji=utf8 sum.tex
pbibtex --kanji=utf8 sum.aux
platex --kanji=utf8 sum.tex
mendex sum.nlo -s nomencl.ist -o sum.nls
platex --kanji=utf8 sum.tex
dvipdfmx sum.dvi
start sum.pdf
